#include <iostream>
#include <cmath>

bool isPrime(int n) {
    if (n <= 1) return false;
    for (int i = 2; i <= std::sqrt(n); i++) {
        if (n % i == 0) return false;
    }
    return true;
}

int main() {
    int number = 29;
    std::cout << number << (isPrime(number) ? " is prime" : " is not prime") << std::endl;
    return 0;
}
